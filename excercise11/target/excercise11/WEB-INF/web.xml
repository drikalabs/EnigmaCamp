<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>HelloServlet</servlet-name>
    <servlet-class>com.enigma.Hello</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>HelloServlet2</servlet-name>
    <servlet-class>com.enigma.ApaKabar</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Robot</servlet-name>
    <servlet-class>com.enigma.RobotMovement</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>HelloServlet</servlet-name>
    <url-pattern>/helo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>HelloServlet2</servlet-name>
    <url-pattern>/pakabar</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Robot</servlet-name>
    <url-pattern>/robot</url-pattern>
  </servlet-mapping>
</web-app>
